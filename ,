get_names(){
    for i in $(ls -d $1/*/);do
            echo -en "$(basename $i)/ ";
    done
}
_cd(){
    #compdef  foo    
    . /usr/share/zsh/functions/Completion/Zsh/_cd

    for vp in $EST_CDPLUG_VIRTUAL_PATH;do
        A=($(get_names $vp))
        compadd -J $vp -X $vp -x $(echo -e "\e[35m"$vp"\e[0m") -a A
    done
}
_cd "$@"
